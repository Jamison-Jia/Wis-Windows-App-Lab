    <Color x:Key="LinerGradientColor11">#0eb1b1b1</Color>    <!-- rgba(177, 177, 177, 0.06) -->

    <SvgImageSource x:Key="SettingsImage24"
                    UriSource="ms-appx:///WinUI3.ThemeLibrary/Resources/Images/settings-24-dark.svg" />
    <SvgImageSource x:Key="ToggleSwitchOnImage16"
                    UriSource="ms-appx:///Images/toggle_swtich_on_16_dark.svg" />
